{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "nuxt typecheck",
    "build": "nuxt build",
    "stop": "npx kill-port --port 8080,9000,9099,5001,8085,3000,5002,4000",
    "emulators": "firebase emulators:start --project demo-koryta-pl --only auth,functions,firestore,database",
    "seed": "npx tsx scripts/seed-emulator.ts",
    "dev": "export USE_EMULATORS=true FIRESTORE_EMULATOR_HOST='127.0.0.1:8080' FIREBASE_AUTH_EMULATOR_HOST='127.0.0.1:9099'; nuxt dev --loglevel=verbose",
    "dev:local": "npm run stop; concurrently \"npm run emulators\" \"npm run seed; npm run dev\"",
    "dev:build": "npm run stop; concurrently \"npm run emulators\" \"export USE_EMULATORS=true FIRESTORE_EMULATOR_HOST='127.0.0.1:8080' FIREBASE_AUTH_EMULATOR_HOST='127.0.0.1:9099'; nuxt build; npm run seed; nuxt preview\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:ci": "vitest run",
    "test:e2e": "start-server-and-test dev:local http://localhost:3000 'cypress run'",
    "test:e2e:open": "start-server-and-test dev:local http://localhost:3000 'cypress open'",
    "test:e2e:build": "start-server-and-test dev:build http://localhost:3000 'cypress run'",
    "backstop:test": "backstop test",
    "backstop:approve": "backstop approve",
    "test:visual": "start-server-and-test dev:build http://localhost:3000 'backstop test'",
    "test": "npm run test:ci && npm run test:e2e",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:duplication": "jscpd app server --ignore app/coverage",
    "quick-check": "npm run format; npm run format:check && npm run test:ci && npm run typecheck && npx eslint && npm run check:duplication"
  },
  "dependencies": {
    "@firebase/app-types": "^0.9.3",
    "@nuxt/content": "^3.6.3",
    "@nuxt/eslint": "^1.7.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.11.10",
    "@pinia/nuxt": "^0.11.2",
    "@sentry/nuxt": "^10.11.0",
    "@unhead/vue": "^2.0.13",
    "@vueuse/firebase": "^13.6.0",
    "better-sqlite3": "^12.2.0",
    "d3-force": "^3.0.0",
    "digraph-js": "^2.2.3",
    "eslint": "^9.32.0",
    "eslint-plugin-cypress": "^5.2.1",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^6.4.0",
    "nuxt": "^4.0.1",
    "nuxt-vuefire": "^1.1.0",
    "string-similarity": "^4.0.4",
    "v-network-graph": "^0.9.21",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.8.0",
    "vuefire": "^3.2.2",
    "vuetify-nuxt-module": "^0.16.1"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^7.1.0",
    "@cypress/vue": "^6.0.2",
    "@nuxt/test-utils": "^3.19.2",
    "@percy/cli": "^1.30.1",
    "@percy/cypress": "^3.1.2",
    "@simonsmith/cypress-image-snapshot": "^10.0.3",
    "@types/d3-force": "^3.0.10",
    "@types/string-similarity": "^4.0.2",
    "@types/wait-on": "^5.3.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "backstopjs": "^6.3.25",
    "concurrently": "^9.2.1",
    "cypress": "^15.7.0",
    "eslint-config-prettier": "^10.1.8",
    "firebase-tools": "^15.1.0",
    "happy-dom": "^20.0.11",
    "jscpd": "^4.0.5",
    "kill-port": "^2.0.1",
    "prettier": "^3.8.1",
    "start-server-and-test": "^2.1.3",
    "unplugin-auto-import": "^21.0.0",
    "vite-plugin-eslint2": "^5.0.4",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@vercel/nft": "^0.27.4",
    "minimatch": "^10.2.1"
  }
}
