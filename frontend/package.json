{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "emulators": "firebase emulators:start --project demo-koryta-pl",
    "seed": "npx tsx scripts/seed-emulator.ts",
    "dev": "nuxt dev",
    "dev:local": "concurrently \"npm run emulators\" \"npx wait-on tcp:8080 && npm run seed && export FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 && export FIREBASE_AUTH_EMULATOR_HOST=127.0.0.1:9099 && USE_EMULATORS=true npm run dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "npx vitest",
    "test:integration": "npx vitest -c vitest.integration.config.ts",
    "test:ci": "vitest run",
    "test:e2e": "npx cypress run",
    "test:e2e:local": "CYPRESS_USE_EMULATORS=true npx cypress run"
  },
  "dependencies": {
    "@firebase/app-types": "^0.9.3",
    "@nuxt/content": "^3.6.3",
    "@nuxt/eslint": "^1.7.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.11.10",
    "@nuxt/test-utils": "^3.19.2",
    "@pinia/nuxt": "^0.11.2",
    "@sentry/nuxt": "^10.11.0",
    "@unhead/vue": "^2.0.13",
    "@vueuse/firebase": "^13.6.0",
    "better-sqlite3": "^12.2.0",
    "d3-force": "^3.0.0",
    "digraph-js": "^2.2.3",
    "eslint": "^9.32.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "nuxt": "^4.0.1",
    "nuxt-vuefire": "^1.1.0",
    "string-similarity": "^4.0.4",
    "v-network-graph": "^0.9.21",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.8.0",
    "vuefire": "^3.2.2",
    "vuetify-nuxt-module": "^0.16.1"
  },
  "devDependencies": {
    "@percy/cli": "^1.30.1",
    "@percy/cypress": "^3.1.2",
    "@simonsmith/cypress-image-snapshot": "^10.0.3",
    "@types/d3-force": "^3.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "cypress": "^15.7.0",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "^20.0.11",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@vercel/nft": "^0.27.4"
  }
}
