rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /nodes/{nodeId} {
      allow read: if true; 
    }
    match /edges/{edgeId} {
      allow read: if true;
    }

    match /users/{userId} {
      allow read, write: if request.auth.uid == userId;
    }
  }
}